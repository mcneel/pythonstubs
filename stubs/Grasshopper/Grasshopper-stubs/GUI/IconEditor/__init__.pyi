class IGH_IconObjectGrip:
    @property
    def Index(self) -> int: ...
    @property
    def Owner(self) -> IGH_IconObject: ...
    @property
    def Position(self) -> PointF: ...
    @property
    def Selected(self) -> bool: ...
    @property
    def Tag(self) -> Object: ...
    def RenderGrip(self, G: Graphics) -> None: ...
    @Position.setter
